    "title": "Footer 2",
    "props": {
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "blockClass": ["footer--bottom", "container"]
    },
    "children": ["rich-text#copyright", "custom-div#auaha"]
  },
  "custom-div#auaha": {
    "title": "Logos footer",
    "children": ["image#auaha", "image#vtex"],
