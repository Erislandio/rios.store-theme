  "rich-text#shelf-title": {
    "title": "Titulo da prateleira",
    "props": {
      "blockClass": ["shelf-title"],
      "text": "Promoções da Semana"
    }
  },

  "rich-text#shelf-title-2": {
    "title": "Titulo da prateleira 2",
    "props": {
      "blockClass": ["shelf-title", "brands"],
