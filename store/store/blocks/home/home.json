  "rich-text#shelf-title-3": {
    "title": "Titulo marcas",
    "props": {
      "blockClass": ["shelf-title"],
      "text": "Conheca nossas marcas"
    }
  },

  "slider-layout#shelf-default": {
    "title": "Layout da prateleira",
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 4,
        "phone": 2
      },
      "infinite": true,
      "fullWidth": false,
      "showPaginationDots": "never",
      "showNavigationArrows": "desktopOnly",
      "blockClass": ["shelf"]
    }
  }
