{
  "store.home": {
    "blocks": [
      "list-context.image-list#main-banner",
      "custom-deals#custom-deals",
      "__fold__.experimentalLazyAssets",
      "__fold__",
      "custom-category-list#category-list",
      "rich-text#shelf-title",
      "list-context.product-list#prateleira1",
      "custom-middle-banners#middle-banners"
    ]
  },

  "custom-middle-banners#middle-banners": {
    "title": "Banners do meio",
    "props": {
      "items": [
        {
          "__editorItemTitle": "Banner 1",
          "image": "assets/middle1.jpg",
          "link": "/"
        },
        {
          "__editorItemTitle": "Banner 2",
          "image": "assets/middle2.jpg",
          "link": "/"
        },
         {
           "__editorItemTitle": "Banner 3",
           "image": "assets/middle3.jpg",
           "link": "/"
         }
      ]
    }
  },

  "custom-category-list#category-list": {
    "props": {
      "title": "MARCA DE ROUPAS COM EXCELÊNCIA EM CADA COSTURA E ACABAMENTO.",
      "items": [
        {
          "__editorItemTitle": "Saias",
          "link": "/saias",
          "imageUrl": "assets/categoria1.png"
        },
        {
          "__editorItemTitle": "Sobreposição",
          "link": "/saias",
          "imageUrl": "assets/categoria2.png"
        },
        {
          "__editorItemTitle": "Calças",
          "link": "/calcas",
          "imageUrl": "assets/categoria3.png"
        },
        {
          "__editorItemTitle": "Saias",
          "link": "/saias",
          "imageUrl": "assets/menu.png"
        },
        {
          "__editorItemTitle": "Sobreposição",
          "link": "/saias",
          "imageUrl": "assets/categoria2.png"
        },
        {
          "__editorItemTitle": "Calças",
          "link": "/calcas",
          "imageUrl": "assets/categoria3.png"
        }
      ]
    }
  },

  "custom-deals#custom-deals": {
    "props": {
      "items": [
        {
          "icon": "assets/icons/frete.svg",
          "__editorItemTitle": "Entrega em até 48h",
          "description": "Para Fortaleza"
        },
        {
          "icon": "assets/icons/bar.svg",
          "__editorItemTitle": "Cupons Ativos",
          "description": "Conheça e aproveite!"
        },
        {
          "icon": "assets/icons/card.svg",
          "__editorItemTitle": "Pontocard",
          "description": "Muitas Vantagens"
        },
        {
          "icon": "assets/icons/stores.svg",
          "__editorItemTitle": "Nossas Lojas",
          "description": "Visite nossas filiais"
        }
      ]
    }
  },
  "list-context.product-list#prateleira1": {
    "title": "Prateleira 1",
    "blocks": ["product-summary.shelf#default"],
    "children": ["slider-layout#shelf-default"],
    "props": {
      "blockClass": "shelf",
      "orderBy": "OrderByTopSaleDESC"
    }
  },

  "rich-text#shelf-title": {
    "title": "Titulo da prateleira",
    "props": {
      "blockClass": [
        "shelf-title"
      ],
      "text": "Promoções da Semana"
    }
  },

  "slider-layout#shelf-default": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 4,
        "phone": 2
      },
      "infinite": true,
      "fullWidth": false,
      "showPaginationDots": "never",
      "showNavigationArrows": "desktopOnly",
      "blockClass": ["shelf"]
    }
  }
}
