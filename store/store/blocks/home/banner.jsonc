  "list-context.image-list#main-banner-footer": {
    "title": "Banner home [FOOTER]",
    "children": [
      "slider-layout#main-banner--footer"
    ],
    "props": {
      "experimentalSetExplicitDimensions": true,
      "height": "700px",
      "preload": true,
      "autoplay": {
        "timeout": 5000,
        "stopOnHover": true
      },
      "images": [
        {
          "image": "/arquivos/footer.png",
