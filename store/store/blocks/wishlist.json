{
  "my-account-link.wishlist-link": {
    "props": {
      "label": "Lista de desejos"
    }
  },
  "my-account-page.wishlist-page": {
    "props": {
      "title": "Lista de desejos"
    },
    "children": ["custom-div#wishlist-container"]
  },
  "store.wishlist": {
    "blocks": ["custom-div#wishlist-container"]
  },

  "custom-div#wishlist-container": {
    "props": {
      "blockClass": "wishlist-container",
      "display": "grid",
      "gap": "10px"
    },
    "children": ["list-context.wishlist"]
  },

  "list-context.wishlist": {
    "blocks": ["product-summary.shelf#default"],
    "children": ["list-context-renderer#wrapped-wishlist"]
  },

  "list-context-renderer#wrapped-wishlist": {
    "props": {
      "blockClass": ["wishlist"]
    }
  }
}
